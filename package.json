{
  "name": "minerbeans",
  "version": "0.0.3",
  "description": "Command-line tools for crypto mining accounting.",
  "main": "index.js",
  "engines": {
    "node": ">=8.1.0 ",
    "npm": ">=5.1.0"
  },
  "scripts": {
    "changelog": "node ./node_modules/.bin/changelog git+https://github.com/nottoseethesun/minerbeans.git all --markdown > CHANGELOG.md",
    "profitloss": "node ./src/index.js",
    "test": "npm run lint && ./test.sh",
    "lint": "node ./node_modules/.bin/eslint --config ./.eslintrc.js ./src/*.js",
    "prettier": "node ./node_modules/.bin/prettier ./src/*js --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nottoseethesun/minerbeans.git"
  },
  "keywords": [
    "crypto",
    "miner",
    "mining",
    "cost",
    "accounting",
    "profit",
    "loss"
  ],
  "author": "Chris M. Balz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nottoseethesun/minerbeans/issues"
  },
  "homepage": "https://github.com/nottoseethesun/minerbeans#readme",
  "devDependencies": {
    "changelog": "^1.4.2",
    "eslint": "^4.19.1",
    "prettier": "^1.12.1"
  },
  "dependencies": {
    "colors": "^1.2.4",
    "optimist": "^0.6.1",
    "prompt": "^1.0.0"
  }
}
